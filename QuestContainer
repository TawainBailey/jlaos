	local function hasArtifactInInventory(artifactName)	local function hasArtifactInInventory(artifactName)

		local items = config.getFishInventory()		local items = config.getFishInventory()

		for _, item in ipairs(items) do		for _, item in ipairs(items) do

			local itemData = ItemUtility.GetItemDataFromItemType("Items", item.Id)			local itemData = ItemUtility.GetItemDataFromItemType("Items", item.Id)

			if itemData and itemData.Data.Name == artifactName then			if itemData and itemData.Data.Name == artifactName then

				return true, item				return true, item

			end			end

		end		end

		return false, nil		return false, nil

	end	end



	local function isArtifactPlaced(artifactName)	local function isArtifactPlaced(artifactName)

		local success, templeLevers = pcall(function()		local success, templeLevers = pcall(function()

			return replion:Get({"TempleLevers"})			return replion:Get({"TempleLevers"})

		end)		end)

		if success and templeLevers then		if success and templeLevers then

			return templeLevers[artifactName] or false			return templeLevers[artifactName] or false

		end		end

		return false		return false

	end	end



	local function equipArtifact(artifactName)	local function equipArtifact(artifactName)

		local items = config.getFishInventory()		local items = config.getFishInventory()

		for _, item in ipairs(items) do		for _, item in ipairs(items) do

			local itemData = ItemUtility.GetItemDataFromItemType("Items", item.Id)			local itemData = ItemUtility.GetItemDataFromItemType("Items", item.Id)

			if itemData and itemData.Data.Name == artifactName then			if itemData and itemData.Data.Name == artifactName then

				pcall(function()				pcall(function()

					local equipRemote = ReplicatedStorage.Packages._Index["sleitnick_net@0.2.0"].net["RE/EquipGear"]					local equipRemote = ReplicatedStorage.Packages._Index["sleitnick_net@0.2.0"].net["RE/EquipGear"]

					equipRemote:FireServer(item.UUID)					equipRemote:FireServer(item.UUID)

				end)				end)

				return true				return true

			end			end

		end		end

		return false		return false

	end	end



	local function placeArtifact(artifactName)	local function placeArtifact(artifactName)

		pcall(function()		pcall(function()

			REPlaceLeverItem:FireServer(artifactName)			REPlaceLeverItem:FireServer(artifactName)

		end)		end)

	end	end



