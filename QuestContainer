						lastNotifiedArtifact = artifactName						lastNotifiedArtifact = artifactName

					end					end



					equipArtifact(artifactName)					equipArtifact(artifactName)

					task.wait(0.5)					task.wait(0.5)



					teleportToLocation(locations.lever)					teleportToLocation(locations.lever)

					task.wait(1)					task.wait(1)



					placeArtifact(artifactName)					placeArtifact(artifactName)

